!   Terminal
URxvt.scrollBar: false
URxvt.font: xft:Roboto Mono for Powerline:size=11:style=Regular
URxvt.dynamicColors: true
URxvt.internalBorder: 20
Xcursor.size: 8


!   Rofi
rofi.font: RobotoMono 10
rofi.location: 0
rofi.lines: 9
rofi.width: 25
rofi.padding: 15
rofi.bw: 2
rofi.hide-scrollbar: True
rofi.separator-style: none
!   main window     bg          fg
rofi.color-window:  #101010,    #797880
!   rows            bg          fg          bgalt       hlbg        hlfg
rofi.color-normal:  #101010,    #444444,    #101010,    #202020,    #FFFFFF
